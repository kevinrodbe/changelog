{
  "name": "ch1",
  "version": "22.1.0",
  "description": "my repo",
  "host": "github.com",
  "main": "index.js",
  "homepage": "https://github.com/kevinrodbe/changelog#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinrodbe/changelog.git"
  },
  "issue": "https://github.com/kevinrodbe/changelog/bugs",
  "bugs": {
    "url": "https://github.com/kevinrodbe/changelog/issues"
  },
  "scripts": {
    "preparep": "husky install",
    "release": "HUSKY=0 standard-version",
    "pre": "npm run release -- --prerelease",
    "cm": "cz"
  },
  "keywords": [],
  "author": "Kevin RodBe",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "semantic-release": "^19.0.2",
    "standard-version": "^9.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "types": {
        "chore": {
          "description": "ğŸ¤–  Build process or auxiliary tool changes",
          "value": "chore"
        },
        "ci": {
          "description": "ğŸ¡  CI related changes",
          "value": "ci"
        },
        "docs": {
          "description": "ï¸âœï¸  Documentation only changes",
          "value": "docs"
        },
        "feat": {
          "description": "ğŸ¸  A new feature",
          "value": "feat"
        },
        "fix": {
          "description": "ğŸ›  A bug fix",
          "value": "fix"
        },
        "perf": {
          "description": "ï¸âš¡  A code change that improves performance",
          "value": "perf"
        },
        "refactor": {
          "description": "ğŸ’¡  A code change that neither fixes a bug or adds a feature",
          "value": "refactor"
        },
        "release": {
          "description": "ğŸ¹  Create a release commit",
          "value": "release"
        },
        "style": {
          "description": "ğŸ’„  Markup, white-space, formatting, missing semi-colons...",
          "value": "style"
        },
        "test": {
          "description": "ğŸ’  Adding missing tests",
          "value": "test"
        }
      }
    }
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/release-notes-generator",
        {
          "presetConfig": {
            "changelogHeader": "# Hi\n\n bais bla bla ![Insomnia API Client](https://raw.githubusercontent.com/Kong/insomnia/develop/screenshots/main.png)",
            "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{previousTag}}...{{currentTag}}--{{user}}",
            "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/bug/{{id}}",
            "issuePrefixes": [
              "#",
              "SC-"
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Hi\n\n again pipol ğŸš€ ![Insomnia API Client](https://raw.githubusercontent.com/Kong/insomnia/develop/screenshots/main.png)"
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
